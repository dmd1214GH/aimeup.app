{
  "name": "aimeup",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.14.0",
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*",
    "packages/eatgpt/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier -w .",
    "dev:web": "pnpm --filter @aimeup/eatgpt-web dev",
    "dev:native": "cd apps/eatgpt-native && npx react-native start",
    "dev:ladle": "pnpm --filter @aimeup/core-ui ladle",
    "build:web": "pnpm --filter @aimeup/eatgpt-web build",
    "build:native": "echo build native (assembleDebug later)",
    "build:ladle": "echo ladle has no build step"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "18.3.7",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18.3.7",
    "@types/react-dom": "18.3.0"
  }
}