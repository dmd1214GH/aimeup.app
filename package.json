{
  "name": "aimeup",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": { "node": "22.18.x" },
  "scripts": {
    "build": "turbo run build && pnpm run cache:prune",
    "dev": "turbo run dev --parallel",
    "hygiene": "turbo run hygiene",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "check": "turbo run hygiene typecheck test",
    "clean": "turbo run clean && rm -rf node_modules && pnpm clean:logs",
    "clean:cache": "rm -rf .temp/turbo-cache",
    "clean:logs": "find . -type d -name '.turbo' -exec rm -rf {} + 2>/dev/null || true",
    "cache:prune": "[ -d .temp/turbo-cache ] && [ $(find .temp/turbo-cache -type d -name '[0-9a-f]*' | wc -l) -gt 50 ] && find .temp/turbo-cache -type d -name '[0-9a-f]*' | head -n -50 | xargs rm -rf || true",
    "ci:install": "pnpm install --frozen-lockfile"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-typescript": "^7.25.0",
    "@types/node": "22.8.7",
    "babel-jest": "^29.5.0",
    "eslint": "9.33.0",
    "jest": "^29.5.0",
    "prettier": "3.6.2",
    "turbo": "^2.4.0",
    "typescript": "5.9.2"
  }
}
