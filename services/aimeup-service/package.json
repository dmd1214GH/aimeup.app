{
  "name": "@aimeup/service",
  "version": "0.0.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && firebase emulators:start --only functions",
    "deploy": "firebase deploy --only functions",
    "hygiene": "eslint . --ext .ts,.tsx,.js,.jsx --ignore-pattern node_modules --max-warnings 0 || true",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "@aimeup/core": "workspace:*",
    "@aimeup/helpers": "workspace:*",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0",
    "pino": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.0"
  }
}
