{
  "name": "@aimeup/eatgpt",
  "version": "0.0.0",
  "description": "EatGPT application",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build": "echo 'Expo app build ready'",
    "dev": "expo start --port 8082",
    "start": "npx expo start --port 8082",
    "start:clean": "npx expo start --clear --port 8082",
    "start:web": "npx expo start --web --port 8082",
    "stop": "lsof -ti:8082 | xargs kill || echo 'No server running'",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --port 8082",
    "web:ci": "BROWSER=none expo start --web --port 8082",
    "hygiene": "eslint . || true",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:smoke:web": "playwright test hello-world.spec.ts --project=chromium --timeout=30000",
    "test:smoke:mobile": "cd __tests__/e2e/maestro && maestro test hello-world.flow.yaml",
    "test:e2e:web:all": "playwright test --project=chromium --timeout=30000",
    "clean": "rm -rf .expo dist test-results playwright-report .temp/maestro"
  },
  "dependencies": {
    "@expo/metro-runtime": "5.0.4",
    "@react-navigation/native": "7.1.17",
    "expo": "~53.0.20",
    "expo-asset": "~11.1.7",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-keep-awake": "~13.0.2",
    "expo-router": "~4.0.14",
    "expo-splash-screen": "~0.29.20",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.1.1",
    "nativewind": "^4.0.1",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-native": "0.79.2",
    "react-native-reanimated": "4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.20.0",
    "tailwindcss": "4.0.0-alpha.31"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/compat": "^1.2.3",
    "@playwright/test": "1.54.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.5.5",
    "@types/react": "~19.0.0",
    "eslint": "^9.17.0",
    "eslint-config-expo": "^7.1.2",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.4",
    "typescript": "~5.7.0"
  },
  "expo": {
    "name": "@aimeup/eatgpt",
    "slug": "eatgpt"
  }
}
