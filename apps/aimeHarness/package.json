{
  "name": "@aimeup/aimeharness",
  "version": "0.0.0",
  "description": "Test harness application for AimeUp development and testing",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build": "echo 'Expo app build ready'",
    "dev": "expo start",
    "start": "npx expo start",
    "start:clean": "npx expo start --clear",
    "start:web": "npx expo start --web",
    "stop": "lsof -ti:8081 | xargs kill || echo 'No server running'",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:ci": "BROWSER=none expo start --web",
    "hygiene": "eslint . || true",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:smoke:web": "playwright test smoke-with-testids.spec.ts --project=chromium --timeout=30000",
    "test:smoke:web:headed": "HEADED=true SLOW_MO=500 playwright test smoke-with-testids.spec.ts --project=chromium --timeout=30000",
    "test:interaction:web": "playwright test interaction-with-testids.spec.ts --project=chromium --timeout=30000",
    "test:interaction:web:headed": "HEADED=true SLOW_MO=500 playwright test interaction-with-testids.spec.ts --project=chromium --timeout=30000",
    "test:smoke:web:legacy": "playwright test smoke.spec.ts --project=chromium --timeout=30000",
    "test:interaction:web:legacy": "playwright test interaction.spec.ts --project=chromium --timeout=30000",
    "test:e2e:web:all": "playwright test --project=chromium --timeout=30000",
    "test:e2e:web:all:headed": "HEADED=true SLOW_MO=500 playwright test --project=chromium --timeout=30000",
    "test:e2e:web:webkit": "playwright test --project=webkit --timeout=30000",
    "test:e2e:web:firefox": "playwright test --project=firefox --timeout=30000",
    "test:smoke:mobile": "cd __tests__/e2e/maestro && maestro test smoke.flow.yaml",
    "test:e2e:mobile": "cd __tests__/e2e/maestro && maestro test .",
    "clean": "rm -rf .expo dist test-results playwright-report .temp/maestro"
  },
  "dependencies": {
    "@aimeup/account": "workspace:*",
    "@aimeup/chat": "workspace:*",
    "@aimeup/config": "workspace:*",
    "@aimeup/core-react": "workspace:*",
    "@babel/runtime": "7.28.2",
    "@eatgpt/healthconnect": "workspace:*",
    "@eatgpt/nutrition": "workspace:*",
    "@expo/metro-runtime": "5.0.4",
    "@react-navigation/native": "7.1.17",
    "@tanstack/react-query": "^5.0.0",
    "dotenv": "^16.4.5",
    "expo": "~53.0.20",
    "expo-asset": "~11.1.7",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-keep-awake": "~14.1.4",
    "expo-modules-core": "2.5.0",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "fbjs": "3.0.5",
    "invariant": "2.2.4",
    "pino": "^9.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-elements": "3.4.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "10.3.0",
    "react-native-web": "~0.20.0",
    "react-redux": "^9.0.0",
    "regenerator-runtime": "0.14.1",
    "whatwg-fetch": "3.6.20"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "1.54.2",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jest": "^29.5.0",
    "jest-expo": "^53.0.9",
    "playwright": "1.54.2",
    "react-native-flipper": "0.273.0",
    "redux-flipper": "2.0.3",
    "typescript": "~5.8.3"
  }
}
